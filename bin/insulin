#!/usr/bin/env ruby
# needs to handle passing config. ~/.insulin or somrthing
require 'insulin'
require 'thor'

module Insulin
  class Insulin < Thor
    map "-i" => :ingest

    desc "ingest", "ingest OnTrack CSV export FILE"
    def ingest file
      csv = Insulin::OnTrackCsvFile.new file
      mongo = Insulin::MongoHandle.new
      csv.save_events mongo
    end
  end
end

Insulin::Insulin.start
